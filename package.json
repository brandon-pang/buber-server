{
  "name": "buber-server",
  "version": "1.0.0",
  "description": "Practicing Server for the (B)Uber Clone Code. GraphQL, Typescript, NodeJS",
  "main": "index.js",
  "repository": "https://github.com/brandon-pang/buber-server.git",
  "author": "Brandon Pang <webbug@me.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/mailgun-js": "^0.16.0",
    "@types/node": "^10.5.0",
    "@types/twilio": "^2.11.0"
  },
  "scripts": {
    "predev": "yarn run types",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "copy": "^0.3.2",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "graphql-tools": "^3.0.4",
    "graphql-yoga": "^1.14.10",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.3.0",
    "mailgun-js": "^0.19.0",
    "merge-graphql-schemas": "^1.5.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "pg": "^8.3.3",
    "ts-node": "^7.0.0",
    "tslint-config-prettier": "^1.13.0",
    "twilio": "^3.49.1",
    "typeorm": "^0.2.25",
    "typescript": "^2.9.2"
  }
}
